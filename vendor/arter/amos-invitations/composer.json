{
    "name": "arter/amos-invitations",
    "description": "System to invite persons to platform",
    "keywords": ["amos", "yii2", "invitations"],
    "homepage": "http://www.arter.it/",
    "type": "component",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4.0",
        "arter/amos-admin": "^2.0.1",
        "arter/amos-core": "^1.9.58",
        "arter/amos-dashboard": "^1.8.4",
        "arter/amos-notify": "^1.4.0",
        "phpoffice/phpexcel": "^1.8"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "arter\\amos\\invitations\\": "src"
        }
    },
    "require-dev": {
        "yiisoft/yii2-faker": "*"
    }
}