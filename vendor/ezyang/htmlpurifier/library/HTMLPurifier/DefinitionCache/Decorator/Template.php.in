/**
 * Definition cache decorator template.
 */
class HTMLPurifier_DefinitionCache_Decorator_Template extends HTMLPurifier_DefinitionCache_Decorator
{

    /**
     * @type string
     */
    public $name = 'Template'; // replace this

    public function copy()
